{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-tabs":0,
        "no-plusplus":0,
        "eqeqeq": "off",
        "no-param-reassign": ["error", { "props": false }],
        "camelcase": ["error", {"allow": ["get_all_data", "get_detail_data", "post_review", "object_builder", "Like_Button", "resize_bg"]}],
        "no-underscore-dangle": 0,
        "no-use-before-define": ["error", { "variables": false }],
        "padded-blocks": ["error", "never"],
        "no-console": "off",
        "no-shadow": "off"
    }
}
